{
    "name": "languaje-freefem",
    "displayName": "FreeFem++",
    "description": "FreeFem syntax highlight",
    "version": "0.1.0",
    "publisher": "Paulms",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:FreeFem.executeFile",
        "onLanguage:FreeFem++"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "FreeFem++",
                "aliases": [
                    "FreeFem++",
                    "FreeFem++"
                ],
                "extensions": [
                    ".edp",
                    ".idp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "FreeFem++",
                "scopeName": "source.FreeFem++",
                "path": "./syntaxes/FreeFem++.tmGrammar.json"
            }
        ],
        "commands": [
            {
                "command": "languaje-freefem.executeFile",
                "title": "FreeFem++: Execute File"
            }
        ],
        "keybindings": [
            {
                "command": "languaje-freefem.executeFile",
                "key": "F5",
                "when": "editorTextFocus && editorLangId == FreeFem++"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "FreeFem++",
            "properties": {
                "FreeFem.executablePath": {
                    "type": "string",
                    "default": "",
                    "description": "Points to the FreeFem++ executable.",
                    "scope": "window"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "update2latest": "git pull && git submodule init && git submodule update && npm install && npm run compile"
    },
    "dependencies": {
        "async-child-process": "^v1.1.1",
        "async-file": "^v2.0.2",
        "child-process-promise": "^v2.2.1",
        "promised-temp": "^v0.1.0",
        "vscode-jsonrpc": "^4.0.0",
        "vscode-languageclient": "^5.2.1",
        "which": "^1.3.1"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.18",
        "mocha": "^5.2.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.26"
    }
}
